{
  "name": "myEfree Connect",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Allows you to automatically connect to myEfree using myEfrei SSO",
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "cookies",
    "https://myefree-connect.docsystem.xyz/*",
    "https://www.myefrei.fr/*",
    "https://auth.myefrei.fr/*"
  ]
}